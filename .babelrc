{
  "presets": [
    ["env", 
      {
      "modules": "commonjs",
       //强制开启某些模块，默认为[]
     "include": ["transform-es2015-arrow-functions"],
      "targets": {
        "browsers": ["> 1%", "last 7 versions", "not ie <= 8"]
      }
    }
  ],
    
    "stage-0",
    // ["es2015", { "loose": true }]
    ["es2015"]
  ],
   "plugins": ["transform-runtime","transform-vue-jsx","add-module-exports","transform-es2015-arrow-functions"]
}
